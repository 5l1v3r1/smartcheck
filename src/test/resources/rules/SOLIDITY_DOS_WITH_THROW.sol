pragma solidity ^0.4.5;
contract Auction {
    address addr;
    function bid() payable {
        if (currentLeader.send(highestBid)) { throw; }//637fdc
        if (!currentLeader.send(highestBid)) { revert; } //637fdc

        for(uint x; x < refundAddresses[x].transfer(1 wei); x++) {//efb788
            addr.transfer(1 wei);
        }

        while ( x > refundAddresses[x].transfer(1 wei)) {//04242c
            refundAddresses[x].transfer(1 wei);
        }
    }
}